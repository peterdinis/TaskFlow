{
  "name": "taskflow",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "format:staged": "biome format --write --no-errors-on-unmatched --files-ignore-unknown=true",
    "lint": "biome lint .",
    "lint:fix": "biome lint --apply .",
    "lint:staged": "biome lint --apply --no-errors-on-unmatched --files-ignore-unknown=true",
    "check": "biome check .",
    "check:fix": "biome check --apply .",
    "check:staged": "biome check --apply --no-errors-on-unmatched --files-ignore-unknown=true",
    "ci:check": "biome ci .",
    "pre-commit": "npm run check:staged",
    "prepare": "husky || true",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch --preserveWatchOutput",
    "clean": "rm -rf dist .turbo node_modules/.cache",
    "clean:full": "rm -rf dist .turbo node_modules",
    "dev:analyze": "ANALYZE=true vite build",
    "dev:profile": "vite dev --profile",
    "dev:inspect": "node --inspect-brk node_modules/.bin/vite dev"
  },
